<% content_for :page_title do %>
  <%= h(@budget.name).humanize %> :: <%= target_desc(@furnishings.target).humanize %>
<% end %>
<% content_for :sidenav, render(:partial => 'sidenav') %>

<% unless @furnishings.errors.empty? %>
  <div class="error">
	  <%= error_messages_for :furnishings, :header_message => nil %>
  </div>
<% end %>
<%= form_for :furnishings,
             :url => budget_furnishings_index_path(@budget) do |f| %>
  <%= render :partial => f %>
<% end %>